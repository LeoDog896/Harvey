{
  "env": {
      "es2021": true,
      "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "indent": [
          "error", 4
      ],
      "camelcase": "error",
      "no-duplicate-imports": "error",
      "semi": 2
  }
}
